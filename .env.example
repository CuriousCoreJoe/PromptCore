# PromptCore - Local Development Environment Variables
# Copy this file to .env and fill in the values from your Netlify/Supabase dashboard.

# --- SUPABASE CONFIG ---
# Used by frontend and backend functions
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Service role key (REQUIRED for backend functions to bypass RLS)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# --- GEMINI CONFIG ---
# Your Google AI Studio API Key
GEMINI_API_KEY=AIza...

# --- INNGEST CONFIG ---
# Event key for triggering functions
INNGEST_EVENT_KEY=your-event-key
# Signing key for validating webhooks (get this from Inngest Dashboard)
INNGEST_SIGNING_KEY=your-signing-key

# --- OPTIONAL DEV SETTINGS ---
# Set to true to bypass real Gemini Pro calls and use mock responses (SAVE CREDITS!)
MOCK_AI=false
